#Pie and Line Chart

#pie(x,labels,main,col)

x<-c(5,10,15,20)
labels<-c("a","b","C","d")

pie(x,labels)

pie(x,labels,main="Pie Chart")

pie(x,labels,main="Pie Chart",radius = 1)

colors<-c("blue","green","pink","purple")

pie(x,labels,main="Pie Chart",col=colors,radius=1)

#plot(x,type,main,xlab,ylab,col)

#type p,l,b,o points,lines,both,overplotted


l<-c(10,20,50)

#line not coming check

plot(l,type="p",main="Line chart")

